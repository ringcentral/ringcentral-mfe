{
  "name": "@ringcentral/mfe-builder",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c ../../rollup.config.ts --configPlugin typescript",
    "build:doc": "rimraf docs && typedoc --out docs src/index.ts --plugin typedoc-plugin-markdown --readme none --skipErrorChecking",
    "tsc": "tsc --noEmit",
    "watch": "yarn build -w",
    "ci:test": "jest",
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "test": "jest --watch",
    "release": "yarn build && node ../../scripts/build/build-npm-package.js ./npm-package-options.js"
  },
