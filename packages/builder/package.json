{
  "name": "@ringcentral/mfe-builder",
  "version": "0.1.0",
  "description": "A micro frontends framework for building Web applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c ../../rollup.config.ts --configPlugin typescript",
    "build:doc": "rimraf docs && typedoc --out docs src/index.ts --plugin typedoc-plugin-markdown --readme none --skipErrorChecking",
    "tsc": "tsc --noEmit",
    "watch": "yarn build -w",
    "ci:test": "jest",
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "test": "jest --watch"
  },
  "keywords": [
    "Module Federation",
    "Micro Frontends"
  ],
  "author": "RingCentral",
  "license": "MIT",
  "peerDependencies": {
    "@ringcentral/mfe-shared": "^0.1.0",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "webpack": "^5.75.0"
  },
  "dependencies": {
    "@ringcentral/mfe-shared": "^0.1.0",
    "yargs": "17.6.2"
  }
}
